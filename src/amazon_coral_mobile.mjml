    <mj-title>Amazon Coral Mobile</mj-title>
    <mj-style>
    a {
      text-decoration: underline;
    }

    .content-image img {
      max-width: 300px;
    }

    .misc-image img {
      max-width: 200px;
    }

    .icon img {
      max-width: 60px;
    }
    </mj-style>
    <mj-attributes>
      <mc-section background-color="#ffffff" padding="0px"/>
      <mc-text padding="0px"/>
      <mc-image padding="0px"/>
      <mj-class name="section-desc" color="#637488" align="center" line-height="24px" font-size="16px" />
